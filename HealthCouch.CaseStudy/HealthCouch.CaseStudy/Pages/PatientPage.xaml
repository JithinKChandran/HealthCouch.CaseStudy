<Page x:Class="HealthCouch.CaseStudy.Pages.PatientPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HealthCouch.CaseStudy.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PatientPage">

    <Grid Background="White">
        <TabControl Margin="10">
            <TabItem Header="Manage Patients">
                <StackPanel Margin="10">
                    <Frame x:Name="ManagePatient" Source="pack://application:,,,/Pages/ManagePatientsPage.xaml"></Frame>
                </StackPanel>
            </TabItem>
            
            <TabItem Header="All Patients">
                <StackPanel Margin="10">
                    <Frame x:Name="AllPatient" Source="pack://application:,,,/Pages/AllPatients.xaml"></Frame>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
