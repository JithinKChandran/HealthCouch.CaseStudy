<Window x:Class="HealthCouch.CaseStudy.Windows.PatientPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HealthCouch.CaseStudy.Windows"
        mc:Ignorable="d"
        Title="PatientPage" Height="450" Width="800" Icon="/Windows/logo.png">
    <Grid Background="{StaticResource WindowBackground}">
        <TabControl Margin="10">
            <TabItem Header="Manage Patients">
                <StackPanel Margin="10">
                    <Frame x:Name="ManagePatient" Source="pack://application:,,,/Pages/ManagePatientsPage.xaml"></Frame>
                </StackPanel>
            </TabItem>

            <TabItem Header="All Patients">
                <StackPanel Margin="10">
                    <Frame x:Name="AllPatient" Source="pack://application:,,,/Pages/AllPatients.xaml"></Frame>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
